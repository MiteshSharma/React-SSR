{
    "name": "reactnode",
    "version": "1.0.0",
    "description": "Dashboard Mobile App",
    "main": "index.js",
    "author": "Mitesh",
    "license": "MIT",
    "scripts": {
        "test": "jest",
        "test:watch": "jest --watch",
        "webpack:dev:server": "webpack --config webpack.server.js --watch",
        "webpack:dev:start": "nodemon --watch build --exec node build/bundle.js",
        "webpack:dev:buildClient": "webpack --watch --config webpack.client.js",
        "webpack:prod:webClient": "webpack --config webpack.client.js",
        "webpack:prod:server": "webpack --config webpack.server.js",
        "dev": "npm-run-all --parallel webpack:dev:*",
        "build-dev": "NODE_ENV=development npm-run-all --parallel webpack:prod:*",
        "build": "NODE_ENV=production npm-run-all --parallel webpack:prod:*",
        "stop": "pm2 kill",
        "prod": "pm2 --no-daemon start build/bundle.js"
    },
    "devDependencies": {
        "@babel/cli": "^7.6.0",
        "@babel/core": "^7.6.0",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/plugin-proposal-decorators": "^7.6.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/plugin-transform-object-assign": "^7.2.0",
        "@babel/polyfill": "^7.6.0",
        "@babel/preset-env": "^7.6.0",
        "@babel/preset-react": "^7.0.0",
        "@babel/runtime": "^7.6.0",
        "@types/enzyme": "^3.10.3",
        "@types/enzyme-adapter-react-16": "^1.0.5",
        "@types/express": "^4.17.1",
        "@types/http-proxy-middleware": "^0.19.3",
        "@types/jest": "^24.0.18",
        "@types/material-ui": "^0.21.7",
        "@types/react": "^16.9.2",
        "@types/react-dom": "^16.9.0",
        "@types/react-helmet": "^5.0.10",
        "@types/react-router": "^5.0.3",
        "@types/react-router-dom": "^4.3.5",
        "@types/universal-cookie": "^2.2.0",
        "acorn": "^7.0.0",
        "awesome-typescript-loader": "^5.2.1",
        "babel-core": "^7.0.0-bridge.0",
        "babel-jest": "^25.0.0",
        "babel-loader": "^8.0.6",
        "babel-preset-minify": "^0.5.1",
        "brotli-webpack-plugin": "^1.1.0",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.14.0",
        "enzyme-to-json": "^3.4.0",
        "http-proxy-middleware": "^0.20.0",
        "jest": "^24.9.0",
        "nodemon": "^1.19.2",
        "npm-run-all": "^4.1.5",
        "pm2": "^3.5.1",
        "style-loader": "^1.0.0",
        "ts-jest": "^24.1.0",
        "ts-loader": "^6.1.0",
        "typescript": "^3.6.3",
        "typescript-plugin-styled-components": "^1.4.3",
        "uglifyjs-webpack-plugin": "^2.2.0",
        "webpack": "^4.40.2",
        "webpack-bundle-analyzer": "^3.5.0",
        "webpack-cli": "^3.3.8",
        "webpack-node-externals": "^1.7.2"
    },
    "dependencies": {
        "@types/node": "^13.7.0",
        "body-parser": "^1.19.0",
        "express": "^4.17.1",
        "express-static-gzip": "^2.0.5",
        "react": "^16.9.0",
        "react-dom": "^16.9.0",
        "react-helmet": "^5.2.1",
        "react-router": "^5.0.1",
        "react-router-dom": "^5.0.1",
        "universal-cookie": "^4.0.2"
    }
